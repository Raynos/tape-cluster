{
  "name": "tape-harness",
  "version": "3.2.1",
  "description": "A helper to run integration tests against an application",
  "keywords": [],
  "author": "Raynos <raynos2@gmail.com>",
  "repository": "git://github.com/Raynos/tape-harness.git",
  "main": "index",
  "homepage": "https://github.com/Raynos/tape-harness",
  "bugs": {
    "url": "https://github.com/Raynos/tape-harness/issues",
    "email": "raynos2@gmail.com"
  },
  "contributors": [
    {
      "name": "Raynos"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "nyc": "^14.1.1",
    "opn": "^1.0.1",
    "request": "2.88.0",
    "standardx": "^3.0.1",
    "tap": "14.7.1",
    "tap-spec": "^2.1.1",
    "tape": "^3.4.0"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        "error",
        4
      ]
    }
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/Raynos/tape-harness/raw/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "npm run jshint -s && npm run cover -s",
    "unit-test": "node test/index.js | tap-spec",
    "jshint": "standardx",
    "cover": "nyc --report html --print detail -- tap test/*.js && npm run check-cover -s",
    "check-cover": "nyc check-coverage --branches=100 --lines=100 --functions=100",
    "view-cover": "opn ./coverage/index.html",
    "travis": "npm run cover -s && nyc report lcov"
  },
  "engine": {
    "node": ">= 0.8.x"
  },
  "pre-commit": [
    "test"
  ],
  "pre-commit.silent": true,
  "ngen-version": "5.1.0"
}
